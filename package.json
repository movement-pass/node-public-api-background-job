{
  "name": "movement-pass-public-api-background-job",
  "version": "0.1.0",
  "private": true,
  "main": "build/src/index.js",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "lint": "eslint ./ --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "release": "npm run format && npm run lint && npm run test && npm run build && npm run pack",
    "deploy": "npm run release && cdk deploy --require-approval never --profile movement-pass",
    "destroy": "cdk destroy --profile movement-pass"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.37.0",
    "@aws-sdk/client-ssm": "^3.37.0",
    "@aws-sdk/lib-dynamodb": "^3.37.0",
    "aws-lambda": "^1.0.6",
    "aws-xray-sdk": "^3.3.3",
    "dayjs": "^1.10.7",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/aws-dynamodb": "^1.129.0",
    "@aws-cdk/aws-iam": "^1.129.0",
    "@aws-cdk/aws-lambda": "^1.129.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.129.0",
    "@aws-cdk/aws-sqs": "^1.129.0",
    "@aws-cdk/aws-ssm": "^1.129.0",
    "@aws-cdk/core": "^1.129.0",
    "@types/aws-lambda": "^8.10.84",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vercel/ncc": "^0.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "typescript": "^4.4.4"
  }
}
